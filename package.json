{
  "name": "agencia-criativa-web",
  "version": "1.0.0",
  "scripts": {
    "sass": "sass scss/estilos.scss css/estilos.css",
    "sass:watch": "sass --watch scss/estilos.scss css/estilos.css",
    "postcss": "postcss css/estilos.css -o css/estilos.css",
    "serve": "live-server",
    "dev": "npm-run-all --parallel sass:watch serve",
    "build": "sass scss/estilos.scss css/estilos.css --style=compressed && postcss css/estilos.css -o css/estilos.css"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.27",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.97.3"
  }
}